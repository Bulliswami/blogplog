<%render 'error'%>


<%=form_with(model:@factorie,local:true) do |f| %>
<p>
    <%=f.label:name%><br/>
    <%=f.text_field:name%>
</p>
<p>
    <%=f.label:description%><br/>
    <%=f.text_area:description%>
</p>
<p>
    <%=f.label:location%><br/>
    <%=f.text_field:location%>
</p>
<p>
    <%=f.submit%>
</p>

<%end%>
